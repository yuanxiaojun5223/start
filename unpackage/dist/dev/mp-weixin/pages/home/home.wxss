/* 水平间距 */
page {
  background-color: #fff;
}
.sticky {
  position: -webkit-sticky;
  position: sticky;
  left: 0;
  top: 0;
  z-index: 999;
}
.swiper-box {
  width: 100%;
  height: 340rpx;
}
.swiper-box .swiper-img {
  width: 100%;
  height: 100%;
}
.cate-list {
  margin: 6px 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.cate-list .cate-item {
  width: 128rpx;
}
.floor-list .floor-item {
  margin-bottom: 6px;
}
.floor-list .floor-item .floor-title .floor-title-img {
  width: 100%;
}
.floor-list .floor-item .floor-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 6px;
}
.floor-list .floor-item .floor-content .right {
  margin-left: 6px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}

